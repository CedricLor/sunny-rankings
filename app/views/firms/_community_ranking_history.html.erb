<div class="row">
  <div class="col-xs-12">
    <div id="communityRankingHistory" class="text-wrapper card-block shadowed-box">
      <h4>Community Ranking Trends</h4>
      <table class="table table-striped table-responsive">
        <% avg_ratings_by_test = @firm.avg_ratings_by_test %>
        <% for i in 1..5 do %>
          <tr>
            <td>
              <%= @tests.fetch(i - 1).test_question %>
            </td>
            <td>
              <span class="badge">
                <%#= link_to("#", html_options = { class: "neutral-a-links" }) do %>
                  <i class="fa fa-long-arrow-up fa-lg rotate-neutral"></i>
                <%# end %>
              </span>
            </td>
          </tr>
        <% end %>
      </table>
    </div>
  </div>
</div>
